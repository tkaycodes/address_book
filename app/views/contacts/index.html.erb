<h3>My Contacts</h3>

<table class="table table-hover">

  <tr>
      <th> First Name </th>
      <th> Last Name </th>
      <th> Email </th>
      <th> Phone Number </th>
    <tr>
  <% @contacts.each do |contact|%>
    <tr>
      <td><%= contact.first_name %></td>
      <td><%= contact.last_name %></td>
      <td><%= contact.email %></td>
      <td><%= contact.phone_number %></td>
    </tr>
  <% end %>

</table>